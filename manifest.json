{
  "name": "Placed",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Craigslist house hunting less painful.",
  "options_page": "options.html",
  "content_scripts": [{
      "matches": ["https://mail.google.com/*"],
      "js": ["underscore-min.js", "jquery.js", "gmail.js"],
      "css": ["gmail.css"]
  }, {
      "matches": ["http://*.craigslist.org/*"],
      "js": ["underscore-min.js", "jquery.js", "craigslist.js"]
  }],
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
      "storage"
  ],
  "web_accessible_resources": [
      "map.png"
      ]
}
